{
  "name": "rx-fetch",
  "version": "1.3.3",
  "description": "RxJS-flavored version of HTTP fetch API for node.js",
  "main": "index.js",
  "scripts": {
    "test": "semistandard && mocha",
    "test-travis": "semistandard && ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- -R spec ./test/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tangledfruit/rx-fetch.git"
  },
  "keywords": [
    "rx",
    "rxjs",
    "http",
    "fetch"
  ],
  "author": "tangledfruit <code@tangledfruit.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tangledfruit/rx-fetch/issues"
  },
  "homepage": "https://github.com/tangledfruit/rx-fetch#readme",
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "rx": ">=4.0.7 <5",
    "url-parse": "^1.1.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "co-mocha": "^1.1.2",
    "coveralls": "^2.11.9",
    "istanbul": "^0.4.4",
    "mocha": "^2.4.5",
    "nock": "^8.0.0",
    "rx-to-async-iterator": "^1.1.8",
    "semistandard": "^8.0.0"
  },
  "engines": {
    "node": ">=4",
    "npm": ">=3"
  },
  "engineStrict": true,
  "semistandard": {
    "globals": [
      "before",
      "describe",
      "fetch",
      "it"
    ]
  }
